<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_payment_outbornd_beecy" model="ir.sequence">
            <field name="name">Beecy: Vendor Payment</field>
            <field name="code">payment.outbornd.beecy</field>
            <field name="prefix">PV%(y)s%(month)s</field>
            <field name="use_date_range">True</field>
            <field name="padding">4</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
        <record id="seq_payment_inbornd_beecy" model="ir.sequence">
            <field name="name">Beecy: Customer Payment</field>
            <field name="code">payment.inbornd.beecy</field>
            <field name="prefix">RV%(y)s%(month)s</field>
            <field name="use_date_range">True</field>
            <field name="padding">4</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
    </data>
</odoo>
